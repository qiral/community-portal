{
  "name": "community-portal",
  "version": "1.0.0",
  "author": "",
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-collapsible": "^1.1.11",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "lucide-react": "^0.513.0",
    "next": "15.3.3",
    "postcss": "^8.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.57.0",
    "syncpack": "^13.0.4",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^3.4.0",
    "zod": "^3.25.51",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/recharts": "^1.8.29",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "turbo": "^2.4.0"
  },
  "packageManager": "pnpm@10.12.1",
  "private": true,
  "scripts": {
    "admin:add": "pnpm add $1 -F admin",
    "build": "dotenv -e .env -- turbo build",
    "clean": "turbo clean && rm -rf node_modules",
    "dev": "dotenv -e .env -- turbo dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint",
    "main:add": "pnpm add $1 -F main",
    "sync": "syncpack fix-mismatches && syncpack format",
    "sync:check": "syncpack list-mismatches",
    "sync:format": "syncpack format",
    "sync:list": "syncpack list",
    "test": "turbo test",
    "type-check": "turbo type-check"
  }
}
